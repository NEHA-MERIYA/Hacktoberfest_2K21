import java.util.*;
public class Divisors_of_integer
{
public static void main(String arg[])
{
Scanner sc= new Scanner(System.in);
System.out.print("Enter an integer: ");
int num= sc.nextInt();
int i=0;
System.out.println("Divisors of the given integer are=");
for(i=1;i<=num/2;i++)
{
if(num%i==0)
System.out.println(i);
}
System.out.println(num);
}
}
